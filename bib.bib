@Article{	  boynton1996,
  abstract	= {The linear transform model of functional magnetic
		  resonance imaging (fMRI) hypothesizes that fMRI responses
		  are proportional to local average neural activity averaged
		  over a period of time. This work reports results from three
		  empirical tests that support this hypothesis. First, fMRI
		  responses in human primary visual cortex (V1) depend
		  separably on stimulus timing and stimulus contrast. Second,
		  responses to long-duration stimuli can be predicted from
		  responses to shorter duration stimuli. Third, the noise in
		  the fMRI data is independent of stimulus contrast and
		  temporal period. Although these tests can not prove the
		  correctness of the linear transform model, they might have
		  been used to reject the model. Because the linear transform
		  model is consistent with our data, we proceeded to estimate
		  the temporal fMRI impulse-response function and the
		  underlying (presumably neural) contrast-response function
		  of human V1.},
  author	= {Boynton, G M and Engel, S a and Glover, G H and Heeger, D
