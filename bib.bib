  doi		= {10.1016/j.neuroimage.2013.04.019},
  file		= {:Users/andrew/Documents/Mendeley/Kohler et al. - 2013 -
		  Pattern classification precedes region-average hemodynamic
		  response in early visual cortex.pdf:pdf},
  issn		= {1095-9572},
  journal	= {NeuroImage},
  keywords	= {Early classification,Hemodynamic response,Multi-voxel
		  pattern analysis,Rapid temporal sampling,Temporal
		  dynamics,Visual cortex,fMRI},
  month		= apr,
  pages		= {249--260},
  pmid		= {23587693},
