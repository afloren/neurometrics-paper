issn = {00313203},
journal = {Pattern Recognition},
keywords = {classifier ensembles,functional magnetic resonance imaging (fmri),naive labelling,online classification},
month = jun,
number = {6},
pages = {2101--2108},
title = {{Naive random subspace ensemble with linear classifiers for real-time classification of fMRI data}},
