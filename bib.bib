journal = {Current biology : CB},
keywords = {Adult,Consciousness,Consciousness: physiology,Discriminant Analysis,Evoked Potentials,Evoked Potentials: physiology,Humans,Linear Models,Magnetic Resonance Imaging,Photic Stimulation,Vision Disparity,Vision Disparity: physiology,Visual Cortex,Visual Cortex: physiology},
month = jul,
number = {14},
pages = {1301--7},
pmid = {16051174},
title = {{Predicting the stream of consciousness from activity in human visual cortex.}},
url = {http://dx.doi.org/10.1016/j.cub.2005.06.026 http://www.ncbi.nlm.nih.gov/pubmed/16051174},
volume = {15},
year = {2005}
}
