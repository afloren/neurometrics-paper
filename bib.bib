month = may,
number = {5},
pages = {535--40},
pmid = {19396166},
title = {{Circular analysis in systems neuroscience: the dangers of double dipping.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2841687\&tool=pmcentrez\&rendertype=abstract},
volume = {12},
