  journal	= {Magnetic resonance imaging},
  keywords	= {Algorithms,Animals,Artificial
		  Intelligence,Automated,Brain,Brain:
		  physiology,Computer-Assisted,Humans,Image
		  Processing,Magnetic Resonance Imaging,Magnetic Resonance
		  Imaging: methods,Multivariate Analysis,Pattern
		  Recognition},
  month		= oct,
  number	= {7},
  pages		= {921--34},
  pmid		= {18508219},
